<mvc:View
	controllerName="ui5.codersgrowth.app.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<App
		id="app"
		busy="{/busy}"
		busyIndicatorDelay="{/delay}">
		<f:FlexibleColumnLayout
			id="layout"
			layout="{/layout}"
			backgroundDesign="Translucent">
		</f:FlexibleColumnLayout>
	</App>
</mvc:View>