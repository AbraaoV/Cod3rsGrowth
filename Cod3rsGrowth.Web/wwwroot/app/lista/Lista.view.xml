<mvc:View
   controllerName="ui5.codersgrowth.app.lista.Lista"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <Page title="{i18n>homePageTitle}" class="sapUiResponsiveContentPadding">
      <List headerText="Custom Content" items="{path: '/'}" >
         <CustomListItem>
            <HBox>
               <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                  <Label text="{nome}"/>
                  <Label text="{tipo}"/>
               </VBox>
            </HBox>
         </CustomListItem>
      </List>
   </Page>
</mvc:View>